<form id="authgroup-edit" caption="Berechtigungsgruppe" nav-route="security/authgroup/:id">
  <models>
    <model id="$m_A" web-api-action="base/Security/Authgroup" key="$id" post-on-save="true" />
  </models>
  <content>
    <flex-box flex-direction="column" height="100%">
      <text id="info">Hallo Kollege - ${$m_A.Name}</text>
      <data-context data-context="$m_A">
        <!--<editor id="name" bind-to="Name" c-md="6" grow="1"/>-->
        <data-grid id="dataGridTest" grow="1" height="100%">
          <data-grid-columns>
            <data-grid-column id="colNichts" caption="Nichts" />
            <data-grid-column id="colNichts2" caption="Nichts2" />
          </data-grid-columns>
        </data-grid>
        <editor id="mandator" bind-to="IdMandator" />
      </data-context>
    </flex-box>
  </content>
</form>