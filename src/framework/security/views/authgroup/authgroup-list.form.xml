<form id="authgroup-list" caption="Berechtigungsgruppen" nav-route="security/authgroup" nav-id-parent="settings" nav-is-enabled="true">
  <models>
    <model id="$m_A" web-api-action="base/Security/Authgroup" web-api-expand="{ Mandator: null }" />
  </models>
  <edit-popups>
    <edit-popup id="edit" file="./authgroup-edit-form" />
  </edit-popups>
  <content>
    <data-grid id="authgroups" data-context="$m_A" id-edit-popup="edit" add-shortcuts="true" is-main-list="true">
      <data-grid-columns>
        <data-grid-column id="name" bind-to="Name" sort-index="0" sort-order="asc" />
        <data-grid-column id="mandantor" bind-to="Mandator.Name" caption="Mandant" />
      </data-grid-columns>
    </data-grid>
  </content>
</form>